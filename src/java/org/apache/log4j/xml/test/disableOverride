	
# Read the .functions file
. ../../test/.functions

# If set, allows to skip tests 
declare -i start=$1

WITNESS=witness/disableOverride

function testDO {
  configFile=$1
  d=$2
  echo -n "DisableOverrideTest $TEST " 
  java $d org.apache.log4j.xml.test.DisableOverrideTest $configFile > $OUTPUT
  check $WITNESS.$TEST $OUTPUT; echo " - OK."  
}

TEST=1
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml -Dlog4j.disableOverride
fi

TEST=2
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml -Dlog4j.disableOverride
fi

TEST=3
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml -Dlog4j.disableOverride
fi

TEST=4
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml 
fi

TEST=5
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml 
fi

TEST=6
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml 
fi

TEST=7
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml -Dlog4j.disableOverride
fi

TEST=8
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml -Dlog4j.disableOverride
fi

TEST=9
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml -Dlog4j.disableOverride
fi

TEST=10
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml 
fi

TEST=11
if [ $TEST -ge $start ]; then
  testDO do$TEST.xml 
